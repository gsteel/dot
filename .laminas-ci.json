{
    "extensions": [
        "pcov"
    ],
    "ini": [
        "pcov.enabled=1"
    ],
    "stablePHP": "8.0",
    "additional_checks": [
        {
            "name": "Coverage",
            "job": {
                "command": "vendor/bin/phpunit --coverage-clover coverage.xml"
            }
        }
    ]
}
